{
  "name": "Resume",
  "version": "1.0.0",
  "description": "This project will make it possible to make resume in less than five minutes",
  "main": "server.js",
  "author": "Roqeeb Yusuff",
  "license": "MIT",
  "engines": {
    "node": "14.18.1",
    "yarn": "1.22.15"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-dev": "cd client && yarn dev",
    "client": "cd client && yarn start",
    "server": "env-cmd nodemon ./src/server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn client-dev\" \"yarn server\"",
    "start": "concurrently --kill-others-on-fail \"yarn client\" \"yarn server\"",
    "client:build": "cd client && yarn build"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.2",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "html-pdf": "^3.0.1",
    "morgan": "^1.10.0",
    "pug": "^3.0.2",
    "pug-pdf": "^0.1.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.18"
  }
}
